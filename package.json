  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lgaticaq/bomberos-cl-api/issues"
  },
  "homepage": "https://github.com/lgaticaq/bomberos-cl-api#readme",
  "dependencies": {
    "bluebird": "^3.5.0",
